<h1>Editing Meeting Abstracts</h1>
<%= form_for abstract, :html => { :multipart=>true} do |f| %>
  <%= render "shared/error_messages", :target => abstract %>
  <%= render :partial=>'form', :locals => {:f => f } %>
  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>


<%= render :partial => 'navbar', :locals => {:abstract => abstract} %>
