<h1>New protocol</h1>

<%= form_for(:protocol, :url => protocols_path) do |f| %>
  <%= render "shared/error_messages", :target => @protocol %>
  <%= render :partial => 'form', :locals => {:f => f, :people => @people, :datasets => @datasets}%>
  <p>
    <%= submit_tag "Create" %>
  </p>
<% end %>

<%= link_to 'Back', protocols_path %>
