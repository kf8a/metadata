<% cache do %>
<% @themes.each do |theme| -%>
  <% next if theme.datatables? -%>
	<h2><%= theme.name %></h2>
		<ul>
		<% theme.datasets.each do |dataset| -%>
		<% next unless dataset.on_web %>
		<% dataset.protocols.each do |protocol| -%>
      <li>
        <%= link_to "#{protocol.title}", protocol_path(protocol) %> 
      </li>
		<% end -%>
		<% end -%>
		</ul>
<% end -%>

<%= link_to 'New protocol', new_protocol_path %>
<% end %>
