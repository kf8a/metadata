
<div id='protocols'>
<h1 class='right'>Protocols</h1>
<ul>
  <% @protocols.each do |protocol|%>
  <li class='study-title'>
    <%=link_to protocol.title, protocol_path(protocol) %>
    <p><%=h truncate(strip_html_tags(protocol.abstract), :length => 300) if protocol.abstract%></p>
  </li>
  <% end %>
</ul>
</div>
