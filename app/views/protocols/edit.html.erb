<h1>Editing protocol</h1>

<%= semantic_form_for @protocol,:html => { :multipart => true } do |form| %>
  <%= render "shared/error_messages", :target => @protocol %>
  <%= render :partial => 'form', :locals => {:form => form, :websites => @websites, :people => @people, :datasets => @datasets } %>
  <%= form.actions %>
<% end %>

<%= link_to 'Show', protocol_path(@protocol) %> |
<%= link_to 'Back', protocols_path %>
