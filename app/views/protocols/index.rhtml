<% @themes.each do |theme| -%>
	<h2><%= theme.title %></h2>
		<ul>
		<% theme.datasets.each do |dataset| -%>
		<% next unless dataset.on_web %>
		<% dataset.protocols.each do |protocol| -%>
		<li><%= link_to "#{protocol.name}, V#{protocol.version}", protocol_path(protocol) %> <%= protocol.title %> <%= link_to  'Edit', edit_protocol_path(protocol), {:class => 'edit'} if logged_in? %></li>
		<% end -%>
		<% end -%>
		</ul>
<% end -%>

<br />

<%= link_to 'New protocol', new_protocol_path %>