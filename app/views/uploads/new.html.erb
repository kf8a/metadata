<%= semantic_form_for @upload do |form| -%>
  <%= form.inputs do -%>
    <%= form.input :title, :label => "Data Table Title" %>
    <%= form.input :abstract, :as => :text, :label => 'Abstract' , :input_html =>{ :size =>"30x10" }%>
  <% end -%>
  <%= form.actions do -%>
    <%= form.action :submit %>
  <% end -%>
<% end -%>

