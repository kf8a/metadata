<h1>Listing datatables</h1>

<table>
  <tr>
  </tr>
  
<% for datatable in @datatables %>
  <tr>
    <td><%= link_to 'Show', datatable_path(datatable) %></td>
    <td><%= link_to 'Edit', edit_datatable_path(datatable), {:class => 'edit'} %></td>
    <td><%= link_to 'Destroy', datatable_path(datatable), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New datatable', new_datatable_path %>