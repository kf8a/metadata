<h1>Editing datatable</h1>

  <%= form_for @datatable do |f| %>
  <%= render "shared/error_messages", :target => @datatable %>
  <%= render :partial => 'form', :locals => {:f => f, :theme => @theme, :core_areas => @core_areas, :datatable => @datatable, :studies => @studies, :people => @people, :units => @units} %>
  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Show', datatable_path(@datatable) %> |
<%= link_to 'Back', datatables_path %>
