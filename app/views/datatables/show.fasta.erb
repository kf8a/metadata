<% data = @datatable.approved_data -%>
<% fields = data.fields -%>
<% data.each do |row| -%>
><%= Digest::SHA1.hexdigest(row.values.join(''))[0..7] %> <%= fields.collect {|field| row[field] unless 'sequence' == field}.join(' ')[0..60] %>
<%= row['sequence'].gsub(/(.{1,#{80}})/, "\\1\n") -%>
<% end -%>
