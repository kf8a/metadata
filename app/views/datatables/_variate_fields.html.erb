<%= f.inputs do %>
  <%= f.input :name %>
  <%= f.input :weight, :input_html => { :size => 5 } %>
  <%= f.input :description, :input_html => {:size => '200x1'} %>
  <%= f.input :unit %>
  <%= f.input :measurement_scale, :collection => ['nominal', 'interval', 'ratio', 'datetime'] %>
  <%= f.input :data_type, :collection => ['text','datetime','integer','real','text'] %>
  <%= f.input :max_valid %>
  <%= f.input :min_valid %>
  <%= f.input :date_format %>
  <%= f.input :precision %>
  <%= f.input :missing_value_indicator %>
  <%= link_to_remove_fields "Remove", f %>
<% end %>
