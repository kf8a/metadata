<h1>Editing <%= @study.name%></h1>

<%= form_for(:study, :url => study_path(@study), :html => { :method => :put }) do |f| %>
  <%= render "shared/error_messages", :target => @study %>
  <%= render :partial => 'form', :locals => {:f => f} %>
  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Back', datatables_path %>