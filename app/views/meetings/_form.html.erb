<p>
	<label for='meeting_title'>Title</label>
	<%= f.text_field :title %>
</p>
<p>
	<label for='meeting_date'>Start Date</label>
	<%= f.date_select :date %>
</p>
<p>
	<label for='meeting_date_to'>End Date</label>
	<%= f.date_select :date_to %>
</p>
<p>
	<%= f.select :venue_type_id, @venues %>
</p>
<p>
	<label for='meeting_announcement'>Announcement</label>
	<%= textile_editor 'meeting', :announcement, :size => '60x20'%>
</p>

<%= raw textile_editor_initialize -%>
